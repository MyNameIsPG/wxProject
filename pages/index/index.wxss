/**index.wxss**/
.indexContainer {
  width: 100%;
  height: 100%;
}
.bannerContainer {
  width: 100%;
  height: 430rpx;
}
.bannerContainer image {
  width: 100%;
  height: 100%;
}
.listContainer {
  margin-top: 10px;
  padding: 30px 0;
  overflow: hidden;
  width: 100%;
  background: #fff;
}
.listContainer > view {
  width: 33%;
  float: left;
  margin-bottom: 30px;
  text-align: center;
}
.listContainer > view image {
  width: 60px;
  height: 60px;
  display: block;
  margin: 0 auto 10px;
}
.listContainer > view text {
  color: #333333;
  font-size: 14px;
  font-weight: 600;
}
.telLogo {
  position: fixed;
  right: 0px;
  bottom: 120px;
  width: 45px;
  height: 40px;
}
.telLogo image {
  width: 45px;
  height: 40px;
}