<!--pages/MyEvaluate/MyEvaluate.wxml-->
<import src="../wxStar/wxStar.wxml" />

<view class="MyEvaluateContainer">
    <view class="wxStarBox">
        <text>评分：</text>
        <template class="dd" is="wxStar" data="{{wxStar: wxStar}}" />
    </view>
</view>




<view class='btn-panel'>
  <button class='btn' bindtap='resetStar'>重置</button>
  <button class='btn' bindtap='alertStar'>获取当前星数</button>
</view>